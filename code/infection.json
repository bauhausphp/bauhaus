{
    "bootstrap": "./autoload.php",
    "source": {
        "directories": [ "./packages", "./types" ]
    },

    "minMsi": 100,
    "minCoveredMsi": 100,

    "mutators": {
        "@default": true,
        "Concat": {
            "ignore": [
                "Bauhaus\\Types\\Uri\\Chars",
                "Bauhaus\\Types\\Uri\\Patterns\\*"
            ]
        }
    }
}
